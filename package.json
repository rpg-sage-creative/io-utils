{
	"name": "@rsc-utils/io-utils",
	"version": "0.5.3",
	"license": "LGPL-3.0-or-later",
	"main": "build/index.js",
	"type": "module",
	"scripts": {
		"build": "bash ./node_modules/@rsc-utils/core-utils/scripts/sh/build.sh",
		"release": "bash ./node_modules/@rsc-utils/core-utils/scripts/sh/release.sh",
		"-pretest": "docker compose -f docker-compose.yml up -d",
		"test": "bash ./node_modules/@rsc-utils/core-utils/scripts/sh/test.sh --detectOpenHandles",
		"-posttest": "docker compose -f docker-compose.yml down",
		"test-todos": "bash ./node_modules/@rsc-utils/core-utils/scripts/sh/create-test-js.sh",
		"clean": "rm -rf node_modules; rm -f package-lock.json; npm i; npm run build",
		"refresh-tags": "git tag -l | xargs git tag -d; git fetch --tags"
	},
	"devDependencies": {
		"@types/follow-redirects": "^1.14.4",
		"@types/jest": "^29.5.14",
		"@types/node": "^24.6.1",
		"jest": "^29.7.0",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.956.0",
		"@rsc-utils/core-utils": "rpg-sage-creative/core-utils#v0.13.3",
		"csv-parser": "^3.2.0",
		"exifreader": "^4.33.1",
		"follow-redirects": "^1.15.11",
		"pdf2json": "^3.2.2"
	},
	"exifreader": {
		"include": {
			"file": true,
			"gif": true,
			"jpeg": true,
			"png": true,
			"png_file": true,
			"webp": true
		}
	},
	"jest": {}
}
