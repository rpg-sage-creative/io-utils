{
	"name": "@rpg-sage-creative/io-utils",
	"version": "1.0.0",
	"license": "LGPL-3.0-or-later",
	"main": "build/index.js",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/rpg-sage-creative/io-utils.git"
	},
	"publishConfig": {
		"@rpg-sage-creative:registry": "https://npm.pkg.github.com"
	},
	"scripts": {
		"build": "bash ./node_modules/@rpg-sage-creative/dev-scripts/sh/build.sh",
		"release": "bash ./node_modules/@rpg-sage-creative/dev-scripts/sh/release.sh",
		"-pretest": "docker compose -f docker-compose.yml up -d",
		"test": "bash ./node_modules/@rpg-sage-creative/dev-scripts/sh/test.sh --detectOpenHandles",
		"-posttest": "docker compose -f docker-compose.yml down",
		"create-test-todos": "bash ./node_modules/@rpg-sage-creative/dev-scripts/sh/create-test-js.sh"
	},
	"devDependencies": {
		"@rpg-sage-creative/dev-scripts": "^1.0.0",
		"@types/follow-redirects": "^1.14.4",
		"@types/jest": "^30.0.0",
		"@types/node": "^24.10.4",
		"jest": "^30.2.0",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.956.0",
		"@rsc-utils/core-utils": "github:rpg-sage-creative/core-utils#semver:^1.0.0",
		"csv-parser": "^3.2.0",
		"exifreader": "^4.33.0",
		"follow-redirects": "^1.15.11",
		"pdf2json": "^4.0.0"
	},
	"exifreader": {
		"include": {
			"file": true,
			"gif": true,
			"jpeg": true,
			"png": true,
			"png_file": true,
			"webp": true
		}
	},
	"jest": {}
}
